#include <stdio.h>

int sum(int arr, int n)
{
    int sum = 0;
    for (int i=0; i<n; i++)
    {
        sum += arr[2][2];
    }
    return sum;
}

int main()
{
    int arr[2][2]={10,20,30,40};
    int n = sizeof(arr)/sizeof(arr[0]);
    printf("The sum of the given elements is %d", sum(arr, n));
    return 0;
}
